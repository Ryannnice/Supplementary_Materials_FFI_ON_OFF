{
    "version": 4,
    "beforeBuildTasks": [],
    "afterBuildTasks": [],
    "global": {
        "use-microLIB": true,
        "output-debug-info": "enable"
    },
    "c/cpp-compiler": {
        "optimization": "level-1",
        "one-elf-section-per-function": true,
        "c99-mode": true,
        "C_FLAGS": "--C99",
        "CXX_FLAGS": "--C99",
        "warnings": "all-warnings"
    },
    "asm-compiler": {},
    "linker": {
        "output-format": "elf",
        "xo-base": "",
        "ro-base": "0x08000000",
        "rw-base": "0x20000000",
        "misc-controls": "--diag_suppress=L6329"
    }
}